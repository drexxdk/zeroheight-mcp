# Minimal example environment variables required by the app

# Required: MCP server API key (used to call local MCP endpoints/tools)
ZEROHEIGHT_MCP_ACCESS_TOKEN=

# Required: Supabase service role key (server-side admin operations)
SUPABASE_SERVICE_ROLE_KEY=

# Required: Supabase access token (server/API access)
SUPABASE_ACCESS_TOKEN=

# Required: Public Supabase URL (client and server use)
NEXT_PUBLIC_SUPABASE_URL=

# Required: Zeroheight project URL (e.g. https://your-project.zeroheight.com/p/project-id)
ZEROHEIGHT_PROJECT_URL=

# Optional: Zeroheight project password (leave empty if not used)
ZEROHEIGHT_PROJECT_PASSWORD=

# Copy this file to .env.local and populate the values for local development