{
  "name": "zeroheight-mcp",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "prebuild": "npm run lint && npx -y tsc --noEmit",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "test": "vitest run --reporter verbose",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "scrape-all": "npx tsx scripts/tasks/start-scrape-project.ts",
    "scrape-pages": "npx tsx scripts/tasks/start-scrape-pages.ts",
    "clear-data": "npx tsx scripts/clear-data.ts",
    "inspect-puppeteer": "npx tsx scripts/debug/puppeteer-inspect.ts \"https://designsystem.lruddannelse.dk/10548dffa/p/0833d8-logo\"",
    "test-task-ttl": "npx tsx scripts/e2e/test-task-ttl.test.ts",
    "restart-ts-server": "node -e \"const cp=require('child_process');try{cp.execSync('npx tsc --noEmit',{stdio:'inherit'})}catch(e){};console.log('If using VS Code, run \\\"TypeScript: Restart TS Server\\\" from Command Palette (Ctrl+Shift+P)')\"",
    "supabase-login": "npx -y supabase@2.72.8 login",
    "generate-database-schema": "npx -y supabase@2.72.8 gen types typescript --project-id qyoexslrsblaphbcvjdk --schema public > src/database.schema.ts",
    "generate-database-types": "npx tsx scripts/generate-database-types.ts",
    "generate-database-schemas-and-types": "npm run generate-database-schema && npm run generate-database-types",
    "codemod:guard-record-casts": "npx tsx scripts/codemods/guard-record-casts.ts src --dry-run",
    "inspect-mcp": "npx @modelcontextprotocol/inspector"
  },
  "prepare": "husky install",
  "dependencies": {
    "@base-ui/react": "^1.2.0",
    "@modelcontextprotocol/sdk": "^1.25.2",
    "@sparticuz/chromium": "^143.0.4",
    "@supabase/supabase-js": "^2.39.3",
    "boxen": "^8.0.1",
    "mcp-handler": "^1.0.7",
    "motion": "^12.34.0",
    "next": "16.1.6",
    "puppeteer": "^24.37.2",
    "puppeteer-core": "^24.37.2",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-icons": "^5.5.0",
    "sharp": "^0.34.5",
    "uuid": "^9.0.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@modelcontextprotocol/inspector": "^0.15.0",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/puppeteer": "^5.4.7",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "dotenv": "^17.2.4",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "husky": "^8.0.3",
    "tailwindcss": "^4",
    "ts-morph": "^18.0.0",
    "tsx": "^4.21.0",
    "typescript": "^5",
    "vitest": "^1.0.0"
  }
}
